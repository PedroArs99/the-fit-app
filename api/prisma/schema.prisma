datasource db {
  provider = "mongodb"
  url      = env("MONGO_CONNECTION_STRING")
}

generator client {
  provider = "prisma-client-js"
}

model Excercise {
  id String @id @map("_id") 
  imageUrl String
  name String

  @@map("excercises")
}

model Meal {
  id String @id @map("_id")
  imageUrl String
  name String
  nutritionalValues NutritionalValues

  @@map("meals")
}

type NutritionalValues {
  carbohydrates Float
  energy Float
  fat Float
  fiber Float
  protein Float
  salt Float
}